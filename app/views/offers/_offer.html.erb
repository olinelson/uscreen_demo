<div id="<%= dom_id offer %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= offer.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= offer.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Subtitle:</strong>
    <%= offer.subtitle %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= offer.description %>
  </div>
</div>
